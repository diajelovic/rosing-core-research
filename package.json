{
  "name": "rosing-core",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-core": "yarn rimraf build && webpack --env.prod --config src/core/core.webpack.config.js && yarn build-core-typings",
    "build-core-typings": "yarn rimraf build/typings && tsc --emitDeclarationOnly -d -p src/core/tsconfig.json",
    "build": "yarn build-client && yarn build-server",
    "build-client": "yarn rimraf build/client && webpack --env.prod --config src/app/client/client.webpack.config.js",
    "build-server": "yarn rimraf build/server && webpack --env.prod --config src/app/server/server.webpack.config.js",
    "build-client-dev": "webpack --config src/app/client/client.webpack.config.js",
    "build-server-dev": "webpack --config src/app/server/server.webpack.config.js",
    "start-server": "nodemon build/server/index.js",
    "core-dev": "webpack --config src/core/core.webpack.config.js",
    "server-dev" : "webpack --config src/app/app.webpack.config.js",
    "client-dev": "webpack-dev-server --config src/app/client/client.webpack.config.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "@types/express": "^4.17.6",
    "@types/loadable__component": "^5.10.0",
    "@types/loadable__server": "^5.12.2",
    "@types/lodash": "^4.14.155",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^1.7.2",
    "webpack-stats-plugin": "^0.3.1"
  },
  "dependencies": {
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "core-js": "3",
    "deepmerge": "^4.2.2",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "webpack-merge": "^5.1.1"
  }
}
